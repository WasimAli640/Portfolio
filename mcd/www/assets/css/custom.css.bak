     transition:color 0.25s ease; 
	 z-index:1;
	 color:#fff;
}


.btn-cmn::after {
  position: absolute;
  content: '';
  top:0;
  left: 0;
  width: 0;
  height: 100%;
  background-color:#23a838 !important;
  transform-origin:left;
  transition:width 0.25s ease;
  z-index:-1;
  border-radius:4px;

}
